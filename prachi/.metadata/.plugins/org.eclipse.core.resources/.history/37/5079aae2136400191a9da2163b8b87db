public static void FileWrite() throws IOException
{
	
		@SuppressWarnings("resource")
		FileOutputStream fs=new FileOutputStream("D:\\Hello\\c.txt");
		String content="hello";
		byte[] buffer=content.getBytes();
		fs.write(buffer);
	
	
